<resources>
    <style name="contact_name">
        <item name="android:textSize">20sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#333333</item>
    </style>
    <style name="contact_title_text_middle">
        <item name="android:textSize">16dip</item>
        <item name="android:textColor">#f4f4f4</item>
    </style>
    <style name="contact_time_small">
        <item name="android:textSize">11dp</item>
        <item name="android:textColor">#b2b2b2</item>
    </style>
    <style name="contact_text">
        <item name="android:textSize">14dp</item>
        <!-- <item name="android:textStyle">bold</item> -->
        <item name="android:textColor">#000000</item>
        <item name="android:textColorLink">#3B5998</item>
    </style>
    <style name="contact_description_text">
        <!-- <item name="android:textStyle">bold</item> -->
        <item name="android:textColor">#000000</item>
        <item name="android:textColorLink">#3B5998</item>
        <item name="android:textSize">@dimen/textsize</item>
    </style>
    <style name="contact_title">
        <item name="android:textSize">20dip</item>
        <!-- <item name="android:textStyle">bold</item> -->
        <item name="android:textColor">#000000</item>
        <item name="android:textColorLink">#3B5998</item>
    </style>
</resources>